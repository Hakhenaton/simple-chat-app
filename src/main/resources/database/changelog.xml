<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:pro="http://www.liquibase.org/xml/ns/pro"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
    http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-latest.xsd">
    <changeSet id="dedf598c-809f-11ee-8317-00155d85504c" author="RNZ">
        <createTable tableName="users">
            <column name="id" type="char(36)">
                <constraints primaryKey="true" primaryKeyName="users_id_primary_key" />
            </column>
            <column name="username" type="varchar(25)">
                <constraints unique="true" uniqueConstraintName="users_username_unique_constraint" />
            </column>
            <column name="password" type="varchar(255)">
                <constraints unique="true" uniqueConstraintName="users_password_unique_constraint" />
            </column>
            <column name="role" type="varchar(255)"></column>
        </createTable>
    </changeSet>
</databaseChangeLog>